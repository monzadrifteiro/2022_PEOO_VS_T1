        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EscolaApp"
        mc:Ignorable="d"
        Title="Cad_Prof_TurmaWindow" Height="270" Width="400">
    <Grid>
        <ListBox Name="listProfs" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,10,0,0" Width="370" Height="90" />
        <ListBox Name="listTurmas" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,110,0,0" Width="370" Height="90" />
        <Button Click="ListarClick" Content="Listar" HorizontalAlignment="Left" Margin="10,207,0,0" VerticalAlignment="Top"/>
        <Button Click="CadastrarClick" Content="Cadastrar" HorizontalAlignment="Left" Margin="50,207,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
